{
	"name": "ymtapp-chome-plug",
	"version": "0.0.2",
	"manifest_version": 2,
	"description": "码头appchrome插件",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"background": {
		"scripts": [
			"js/jquery-1.9.1.min.js",
			"js/background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "码头appchrome插件",
		"default_popup": "popup.html"
	},
	"permissions": ["tabs", "http://*/*"],
	"content_scripts": [{
		"matches": ["http://*/*"],
		"js": [
			"js/jquery-1.9.1.min.js",
			"js/content_scripts.js"
		]
	}]
}